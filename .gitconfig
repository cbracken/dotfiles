[diff "vimdiff"]
  command = git-vimdiff
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore_global
[format]
  pretty = format:%C(yellow)%h%C(reset) %C(green)%cr%C(reset) %C(blue)%cn%C(reset) %s
[user]
	name = Chris Bracken
	email = chris@bracken.jp
[alias]
	lol = log --oneline --graph --decorate
[diff]
	mnemonicprefix = true
[merge]
	stat = true
