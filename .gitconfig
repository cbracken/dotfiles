[alias]
  lol = log --pretty=format:'%C(yellow)%h%C(reset) %C(green)%cr%C(reset) %C(blue)%cn%C(reset) %C(yellow)%d%C(reset) %s'
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore_global
[diff]
  mnemonicprefix = true
[diff "vimdiff"]
  command = git-vimdiff
[merge]
  stat = true
[push]
  default = simple
[rebase]
	autosquash = true
[user]
  name = Chris Bracken
  email = chris@bracken.jp
