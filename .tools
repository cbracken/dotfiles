# -*- mode: shell-script; -*- vim: set filetype=sh
# ~/.tools

# go version manager
if [[ -f ~/.gvm/scripts/gvm ]]; then
  . ~/.gvm/scripts/gvm
fi

# dart version manager
if [[ -f ~/.dvm/scripts/dvm ]]; then
  . ~/.dvm/scripts/dvm
fi

# node version manager
if [[ -f ~/.nvm/nvm.sh ]]; then
  . ~/.nvm/nvm.sh
fi
